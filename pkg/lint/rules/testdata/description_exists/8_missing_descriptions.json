{
    "$id": "https://example.com/person.schema.json",
    "$schema": "https://json-schema.org/draft/2020-12/schema",
    "type": "object",
    "properties": {
        "addresses": {
            "title": "Addresses",
            "type": "array",
            "items": {
                "type": "object",
                "properties": {
                    "city": {
                        "type": "string"
                    },
                    "state": {
                        "type": "string"
                    },
                    "street": {
                        "type": "string"
                    },
                    "zip": {
                        "type": "string"
                    }
                }
            }
        },
        "age": {
            "type": "integer",
            "minimum": 0
        },
        "firstName": {
            "type": "string"
        },
        "lastName": {
            "type": "string"
        }
    }
}
